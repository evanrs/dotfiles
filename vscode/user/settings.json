  // "terminal.integrated.gpuAcceleration": "on",
  "terminal.integrated.gpuAcceleration": "on",
  "vscode_vibrancy.opacity": 0,
  "vscode_vibrancy.theme": "Default Light",
  "editor.unicodeHighlight.invisibleCharacters": false,
  "typescript.tsserver.maxTsServerMemory": 5120,
  "eslint.codeActionsOnSave.mode": "problems",
  "eslint.enable": false,
  "terminal.integrated.enableMultiLinePasteWarning": false,
  "editor.foldingImportsByDefault": true,
  "json.format.enable": false,
  "editor.guides.indentation": false,
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.expand": false,
  "explorer.fileNesting.patterns": {
    "package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml",
    "tsconfig.json": "tsconfig.*.json",
    "*.ts": "${capture}.local.ts, ${capture}.local.*.ts,  ${capture}.draft.ts, ${capture}.draft-*.ts, ${capture}.*-draft.ts,  ${capture}.test.ts, ${capture}.js",
    "*.tsx": "${capture}.local.tsx, ${capture}.local.*.tsx,  ${capture}.draft.tsx, ${capture}.draft-*.tsx, ${capture}.*-draft.tsx,  ${capture}.ts, ${capture}.test.ts, ${capture}.test.tsx",
    "*.js": "${capture}.local.js, ${capture}.local.*.js,  ${capture}.draft.js, ${capture}.draft-*.js, ${capture}.*-draft.js,  ${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
    "*.jsx": "${capture}.local.jsx, ${capture}.local.*.jsx,  ${capture}.draft.jsx, ${capture}.draft-*.jsx, ${capture}.*-draft.jsx,  ${capture}.js"
  },
  "oneDarkPro.italic": true,
  "oneDarkPro.vivid": true,
  "oneDarkPro.bold": true,
  "terminal.integrated.env.osx": {
    "FIG_NEW_SESSION": "1"
  },
  "editor.accessibilitySupport": "off",
  "editor.inlineSuggest.enabled": true,
  "workbench.tree.indent": 22,
  "terminal.integrated.persistentSessionScrollback": 10000,
  "terminal.integrated.scrollback": 100000,
  "codesnap.backgroundColor": "rgba(255, 255, 255, 0)",
  "codesnap.shutterAction": "copy",
  "codesnap.transparentBackground": true,
  "codesnap.showLineNumbers": false,
  "codesnap.containerPadding": "3rem",
  "codesnap.boxShadow": "rgba(0, 0, 0, 0.75) 0px 20px 68px -50px",

  "[Log]": {
    "editor.fontSize": 12,
    "editor.lineHeight": 1.5,
    "editor.letterSpacing": -0.45,
    "editor.fontWeight": "700"
  },
  "editor.tabSize": 2,
  "explorer.excludeGitIgnore": true,
  "editor.formatOnSave": true
