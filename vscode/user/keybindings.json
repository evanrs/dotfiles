// Place your key bindings in this file to override the defaults
[
  {
    "key": "shift+cmd+j",
    "command": "search.action.focusNextSearchResult",
    "when": "hasSearchResult"
  },
  {
    "key": "shift+cmd+k",
    "command": "search.action.focusPreviousSearchResult",
    "when": "hasSearchResult"
  },
  {
    "key": "shift+cmd+f",
    "command": "workbench.action.findInFiles",
    "when": "!searchViewletVisible"
  },
  {
    "key": "ctrl+`",
    "command": "-workbench.action.togglePanel"
  },
  {
    "key": "shift+cmd+r",
    "command": "editor.action.rename",
    "when": "editorHasRenameProvider && editorTextFocus && !editorReadonly"
  },
  {
    "key": "cmd+j",
    "command": "-workbench.action.togglePanel"
  },
  {
    "key": "cmd+/ shift+left",
    "command": "workbench.action.positionPanelLeft"
  },
  {
    "key": "cmd+/ shift+right",
    "command": "workbench.action.positionPanelRight"
  },
  {
    "key": "cmd+/ shift+down",
    "command": "workbench.action.positionPanelBottom"
  },
  {
    "key": "shift+cmd+i",
    "command": "-jsonnet.previewToSide",
    "when": "editorFocus"
  },
  {
    "key": "shift+cmd+a",
    "command": "workbench.files.action.showActiveFileInExplorer"
  },
  {
    "key": "cmd+'",
    "command": "workbench.action.focusActiveEditorGroup"
  },
  {
    "key": "ctrl+cmd+\\",
    "command": "workbench.action.toggleSidebarPosition"
  },
  {
    "key": "ctrl+shift+alt+cmd+w",
    "command": "workbench.action.switchWindow"
  },
  {
    "key": "shift+cmd+'",
    "command": "editor.action.revealDefinition",
    "when": "editorHasDefinitionProvider && editorTextFocus && !isInEmbeddedEditor"
  },
  {
    "key": "f12",
    "command": "-editor.action.revealDefinition",
    "when": "editorHasDefinitionProvider && editorTextFocus && !isInEmbeddedEditor"
  },
  {
    "key": "cmd+k shift+cmd+p",
    "command": "peacock.changeColorToRandom"
  },
  {
    "key": "cmd+n",
    "command": "extension.simpleNewFile"
  },
  {
    "key": "alt+cmd+n",
    "command": "-extension.simpleNewFile"
  },
  {
    "key": "alt+cmd+n",
    "command": "workbench.action.files.newUntitledFile"
  },
  {
    "key": "cmd+n",
    "command": "-workbench.action.files.newUntitledFile"
  },
  {
    "key": "ctrl+alt+cmd+n",
    "command": "fileutils.newFile"
  },
  {
    "key": "cmd+/ cmd+,",
    "command": "workbench.action.openWorkspaceSettings"
  },
  {
    "key": "cmd+/ shift+cmd+,",
    "command": "workbench.action.openWorkspaceSettingsFile"
  },
  {
    "key": "cmd+/ cmd+/",
    "command": "editor.action.commentLine",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "cmd+/",
    "command": "-editor.action.commentLine",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "shift+cmd+b",
    "command": "-workbench.action.tasks.build"
  },
  {
    "key": "shift+cmd+b",
    "command": "workbench.view.extension.bookmarks"
  },
  {
    "key": "cmd+/ s",
    "command": "autoHide.toggleHideSideBar"
  },
  {
    "key": "cmd+/ p",
    "command": "autoHide.toggleHidePanel"
  },
  {
    "key": "cmd+/ cmd+=",
    "command": "workbench.action.toggleMaximizedPanel"
  },
  {
    "key": "cmd+/ tab",
    "command": "workbench.action.nextPanelView"
  },
  {
    "key": "cmd+/ shift+tab",
    "command": "workbench.action.previousPanelView"
  },
  {
    "key": "cmd+k ctrl+shift+alt+cmd+left",
    "command": "workbench.action.splitEditor"
  },
  {
    "key": "cmd+k left",
    "command": "-workbench.action.splitEditor"
  },
  /// TERMINAL STUUUFFFS ðŸ˜±
  {
    "key": "cmd+t",
    "command": "workbench.action.terminal.new",
    "when": "terminalFocus && terminalHasBeenCreated"
  },
  {
    "key": "ctrl+shift+`",
    "command": "-workbench.action.terminal.new",
    "when": "terminalEditorFocus || terminalFocus && terminalHasBeenCreated"
  },
  {
    "key": "cmd+/ shift+p",
    "command": "workbench.action.terminal.moveToTerminalPanel"
  },
  {
    "key": "cmd+k up",
    "command": "workbench.action.terminal.clear",
    "when": "terminalEditorFocus || terminalFocus && terminalHasBeenCreated || terminalFocus && terminalProcessSupported"
  },
  {
    "key": "cmd+k",
    "command": "-workbench.action.terminal.clear",
    "when": "terminalEditorFocus || terminalFocus && terminalHasBeenCreated || terminalFocus && terminalProcessSupported"
  },
  {
    "key": "shift+alt+left",
    "command": "workbench.action.terminal.resizePaneLeft",
    "when": "terminalEditorFocus || terminalFocus && terminalHasBeenCreated || terminalFocus && terminalProcessSupported"
  },
  {
    "key": "ctrl+cmd+left",
    "command": "-workbench.action.terminal.resizePaneLeft",
    "when": "terminalEditorFocus || terminalFocus && terminalHasBeenCreated || terminalFocus && terminalProcessSupported"
  },
  {
    "key": "shift+alt+down",
    "command": "workbench.action.terminal.resizePaneDown",
    "when": "terminalEditorFocus || terminalFocus && terminalHasBeenCreated || terminalFocus && terminalProcessSupported"
  },
  {
    "key": "ctrl+cmd+down",
    "command": "-workbench.action.terminal.resizePaneDown",
    "when": "terminalEditorFocus || terminalFocus && terminalHasBeenCreated || terminalFocus && terminalProcessSupported"
  },
  {
    "key": "shift+alt+right",
    "command": "workbench.action.terminal.resizePaneRight",
    "when": "terminalEditorFocus || terminalFocus && terminalHasBeenCreated || terminalFocus && terminalProcessSupported"
  },
  {
    "key": "ctrl+cmd+right",
    "command": "-workbench.action.terminal.resizePaneRight",
    "when": "terminalEditorFocus || terminalFocus && terminalHasBeenCreated || terminalFocus && terminalProcessSupported"
  },
  {
    "key": "shift+alt+up",
    "command": "workbench.action.terminal.resizePaneUp",
    "when": "terminalEditorFocus || terminalFocus && terminalHasBeenCreated || terminalFocus && terminalProcessSupported"
  },
  {
    "key": "ctrl+cmd+up",
    "command": "-workbench.action.terminal.resizePaneUp",
    "when": "terminalEditorFocus || terminalFocus && terminalHasBeenCreated || terminalFocus && terminalProcessSupported"
  },
  {
    "key": "ctrl+cmd+right",
    "command": "-workbench.action.terminal.resizePaneRight",
    "when": "terminalEditorFocus || terminalFocus && terminalHasBeenCreated || terminalFocus && terminalProcessSupported"
  },
  {
    "key": "cmd+; enter",
    "command": "workbench.action.createTerminalEditor"
  },
  {
    "key": "cmd+; shift+enter",
    "command": "workbench.action.createTerminalEditorSide"
  },
  {
    "key": "cmd+; left",
    "command": "workbench.action.createTerminalEditor"
  },
  {
    "key": "cmd+; right",
    "command": "workbench.action.createTerminalEditorSide"
  },
  {
    "key": "cmd+v",
    "command": "workbench.action.terminal.paste",
    "when": "terminalEditorFocus || terminalFocus && terminalHasBeenCreated"
  },
  {
    "key": "cmd+k cmd+k",
    "command": "workbench.action.terminal.clear",
    "when": "terminalEditorFocus || terminalFocus && terminalHasBeenCreated"
  },
  {
    "key": "cmd+; cmd+;",
    "command": "workbench.action.toggleAuxiliaryBar"
  },
  {
    "key": "cmd+; cmd+;",
    "command": "workbench.action.focusAuxiliaryBar",
    "when": "!terminalFocus"
  },
  {
    "key": "cmd+k right",
    "command": "workbench.action.moveEditorToRightGroup"
  },
  {
    "key": "cmd+k shift+right",
    "command": "workbench.action.moveActiveEditorGroupRight"
  },
  {
    "key": "cmd+k right",
    "command": "-workbench.action.moveActiveEditorGroupRight"
  },
  {
    "key": "cmd+k shift+left",
    "command": "workbench.action.moveActiveEditorGroupLeft"
  },
  {
    "key": "cmd+k left",
    "command": "-workbench.action.moveActiveEditorGroupLeft"
  },
  {
    "key": "cmd+k left",
    "command": "workbench.action.moveEditorToLeftGroup"
  },
  {
    "key": "shift+cmd+d",
    "command": "workbench.action.terminal.split",
    "when": "terminalFocus && terminalProcessSupported || terminalFocus && terminalWebExtensionContributedProfile"
  },
  {
    "key": "ctrl+shift+5",
    "command": "-workbench.action.terminal.split",
    "when": "terminalFocus && terminalProcessSupported || terminalFocus && terminalWebExtensionContributedProfile"
  },
  {
    "key": "cmd+w",
    "command": "workbench.action.terminal.kill",
    "when": "terminalFocus"
  },
  {
    "key": "cmd+w",
    "command": "workbench.action.terminal.killEditor",
    "when": "terminalEditorFocus"
  },
  {
    "key": "cmd+w",
    "command": "-workbench.action.terminal.killEditor",
    "when": "terminalEditorFocus && terminalFocus && terminalHasBeenCreated && resourceScheme == 'vscode-terminal' || terminalEditorFocus && terminalFocus && terminalProcessSupported && resourceScheme == 'vscode-terminal'"
  },
  {
    "key": "cmd+; cmd+r",
    "command": "workbench.action.terminal.rename",
    "when": "terminalEditorFocus || terminalFocus"
  },
  {
    "key": "cmd+e cmd+r",
    "command": "fileutils.renameFile"
  },
  {
    "key": "shift+alt+cmd+f",
    "command": "prettier.forceFormatDocument"
  }
]
