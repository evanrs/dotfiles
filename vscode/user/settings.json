  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "window.restoreWindows": "all",
  "window.title": "${activeEditorMedium}${separator}${separator}${rootName}",
  "window.newWindowDimensions": "inherit",
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  "flow.logLevel": "error",
  "debug.internalConsoleOptions": "neverOpen",
  "comments.openPanel": "neverOpen",
  //
  //  file types and stuff
  //
  "files.associations": {
    ".symlink": "shellscript",
    ".yml.symlink": "shellscript",
    ".json.symlink": "shellscript",
    ".hosts": "shellscript",
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[shellscript]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[graphql]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.defaultFormatter": "yzhang.markdown-all-in-one"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[hosts]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  //
  //
  //
  "files.exclude": {
    "**/.classpath": true,
    "**/.DS_Store": true,
    "**/.factorypath": true,
    "**/.git": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.vscode": true,
    "**/dist": true,
    "**/node_modules": true,
    "**/npm-lock.*": true,
    "**/pnpm-lock.*": true,
    "**/yarn.*": true
  },
  "workbench.startupEditor": "none",
  "files.insertFinalNewline": true,
  "git.autofetch": true,
  "extensions.ignoreRecommendations": false,
  "terminal.integrated.allowWorkspaceConfiguration": true,
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.cursorWidth": 3,
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.editor.tabCloseButton": "off",
  "terminal.integrated.commandsToSkipShell": [
    "workbench.action.switchWindow",
    "workbench.action.terminal.resizePaneLeft",
    "workbench.action.terminal.resizePaneRight",
    "workbench.action.terminal.resizePaneUp",
    "workbench.action.terminal.resizePaneDown"
  ],
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter.notebook.ipynb",
    "*.plist": "default"
  },
  "emmet.showExpandedAbbreviation": "never",
  "security.workspace.trust.enabled": false,
  "browser-preview.startUrl": "http://app.localhost:3000",
  // Theme Setup.
  "window.autoDetectColorScheme": true,
  "workbench.preferredDarkColorTheme": "Palenight Operator",
  "workbench.preferredLightColorTheme": "Overflow Light (rainglow)",
  "workbench.preferredHighContrastColorTheme": "SynthWave '84",
  "workbench.colorTheme": "Overflow Light (rainglow)",
  "terminal.integrated.fontSize": 16,
  "terminal.integrated.fontWeight": 600,
  "terminal.integrated.fontWeightBold": 900,
  "workbench.iconTheme": "file-icons",
  "editor.fontFamily": "Operator Mono, Dank Mono, FiraCode-Retina, Fira Code,  Menlo, Monaco, 'Courier New', monospace",
  "terminal.integrated.fontFamily": "Operator Mono, Dank Mono, FiraCode-Retina, Fira Code,  Menlo, Monaco, 'Courier New', monospace",
  "editor.fontSize": 18,
  "editor.lineHeight": 24.65,
  "editor.letterSpacing": 0.5,
  "editor.fontWeight": "900",
  "editor.fontLigatures": true,
  "editor.cursorStyle": "line",
  "editor.cursorWidth": 5,
  "editor.cursorBlinking": "solid",
  "editor.renderWhitespace": "all",
  "workbench.startupEditor": "newUntitledFile",
  "workbench.editor.enablePreview": false,
  "explorer.confirmDragAndDrop": false,
  "files.trimTrailingWhitespace": true,
  "files.trimFinalNewlines": true,
  "prettier.eslintIntegration": true,
  "eslint.run": "onSave",
  // MacOS Only Settings.
  "workbench.fontAliasing": "antialiased",
  "terminal.integrated.macOptionIsMeta": true,
  "workbench.statusBar.feedback.visible": false,
  // SOP's highlight matching tag setting.
  "highlight-matching-tag.styles": {
    "opening": {
      "full": {
        "highlight": "rgba(165, 153, 233, 0.3)"
      }
    }
  },
  // SOP's Import Cost Extension Settings.
  "importCost.largePackageColor": "#EC3A37F5",
  "importCost.mediumPackageColor": "#B362FF",
  "importCost.smallPackageColor": "#B362FF",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "editor.bracketPairColorization.enabled": true,
  "typescript.tsserver.log": "off",
  "explorer.sortOrderLexicographicOptions": "lower",
  "explorer.sortOrder": "type",
  "explorer.incrementalNaming": "smart",
  "explorer.openEditors.visible": 4,
  "terminal.integrated.minimumContrastRatio": 3.5,
  "terminal.external.osxExec": "iTerm.app",
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "typescript.implementationsCodeLens.enabled": true,
  "typescript.locale": "en",
  "typescript.referencesCodeLens.showOnAllFunctions": true,
  "typescript.surveys.enabled": false,
  "editor.renderLineHighlightOnlyWhenFocus": true,
  "autoHide.panelDelay": 0,
  "autoHide.sideBarDelay": 0,
  "editor.formatOnSaveMode": "modificationsIfAvailable",
  "keyboard.touchbar.enabled": false,
  "files.simpleDialog.enable": true,
  "search.quickOpen.history.filterSortOrder": "recency",
  "search.sortOrder": "modified",
  "workbench.editor.showIcons": false,
  "workbench.editor.sharedViewState": true,
  "workbench.editor.scrollToSwitchTabs": true,
  "editor.suggest.insertMode": "replace",
  "editor.suggest.preview": true,
  "editor.suggest.showClasses": false,
  "editor.suggest.showConstants": false,
  "editor.suggest.showConstructors": false,
  "editor.suggest.showStatusBar": true,
  "autoHide.autoHideSideBar": false,
  "autoHide.autoHidePanel": false,
  "terminal.integrated.tabs.enabled": false,
  "workbench.sideBar.location": "right",
  "autoHide.hideOnOpen": false,
  "editor.formatOnSave": true,
